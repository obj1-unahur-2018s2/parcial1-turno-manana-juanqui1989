class Universidad {
	var provincia
	var honorariosRecomendados
	
	method setProvincia(provinciaElegida) {
		provincia = provinciaElegida
	}
	
	method setHonorariosRecomendados(valor) {
		honorariosRecomendados = valor
	}
	method getProvincia(){
		return provincia
	}
	method getHonorariosRecomendados(){
		return honorariosRecomendados
	}
	
}
